{
  "version": "1.0.0",
  "name": "Google Duo",
  "description": "The simple video calling app.",
  "launch_path": "/index.html",
  "icons": {
    "64": "/assets/icons/duo_64.png",
    "128": "/assets/icons/duo_128.png"
  },
  "developer": {
    "name": "Google LLC",
	"url": "http://google.com"
  },
  "type": "certified",
  "origin": "app://duo.google.com",
  "permissions": {
    "audio-capture" : {
      "description": "Required to make audio calls."
    },
    "contacts": {
      "access": "readwrite",
      "description": "Required to import user's contacts list for calling."
    },
    "desktop-notification": {
      "description": "Needed for creating system notifications."
    },
    "push": {
      "description": "Required for sending notifications."
    },
    "serviceworker": {
      "description": "Required for sending notifications."
    },
    "sms": {
      "description": "Required for auto-verification of a registration request."
    },
    "systemXHR": {
      "description": "Required to load remote content."
    },
    "telephony": {
      "description": "Require to auto-populate the user's phone number."
    },
    "video-capture": {
      "description": "Required to make video calls."
    }
  },
  "messages": [{"serviceworker-notification": "/index.html"}],
  "gcm_sender_id": "466216207879",
  "installs_allowed_from": [
    "*"
  ],
  "locales": {
    "en-US": {
      "name": "Google Duo",
      "description": "The simple video calling app."
    }
  },
  "default_locale": "en-US",
  "fullscreen": true
}
